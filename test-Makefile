login:
	sudo su - postgres
	psql

logout:
	exit

settings:
	psql -c "CREATE DATABASE codingnightlive;"
	psql -c "CREATE USER cnluser WITH PASSWORD 'temporary';"
	psql -c "ALTER ROLE cnluser SET client_encoding TO 'utf8';"
	psql -c "ALTER ROLE cnluser SET default_transaction_isolation TO 'read committed';"
	psql -c "ALTER ROLE cnluser SET timezone TO 'UTC-9';"
	GRANT ALL PRIVILEGES ON DATABASE codingnightlive TO cnluser;
